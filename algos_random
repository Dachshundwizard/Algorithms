var people = [
    {
        name:"mike",
        pet_id:3
    },
    {   name:"Michael",
        pet_id:2
    },
    {
        name:"Swetha",
        pet_id:4
    },
    {
        name:"Jack",
        pet_id:1
    }
]
var pets = [
    {
        id:1,
        type: "fish"
    },
    {
        id:2,
        type:"dog"
    },
    {
        id:3,
        type: "cat"
    },
    {
        id:4,
        type: "pygmy elephant"
    }
]
//
function joinBypet_id(people, pets){
    var result = [];
    for (var i = 0; i < people.length; i++){
        var obj = {};
        obj.name = people[i].name;
        for (var j = 0; j < pets.length; j++){
            if (people[i].pet_id == pets[j].id){
                obj.pet = pets[j].type;
            }
        }
        result.push(obj);
    }
    return result;
}
console.log(joinBypet_id(people, pets));
