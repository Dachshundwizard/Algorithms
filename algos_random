function arrayReverse(array){
    var newArray=[]
    for (var i = array.length-1; i >= 0; i--) {
        newArray.push(array[i]);
    }
    newArray
    console.log(newArray);
    return newArray;
}
arrayReverse([1,2,3,4,5]);

//1st challenge reverse string
//2nd challenge figure out how to do this faster, without creating new array
function arrayReverse(array){
        array.reverse();
        console.log(array);
}
arrayReverse([1,2,3,4,5]);
