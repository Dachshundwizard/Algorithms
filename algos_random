//     key:"value"
// }
//
// myDictionary["key"] or
// myDictionary.key
//
// //They can only have the key once. Cannot have multiples of the same key in same dictionary.
// // 3rd callenge, given a string, return a string or array or dict with just the letters that occur once.
//

//object = {}
//object[key] = value
//object[key] <---will give you back the value
//e.g. an array[0] would give you back the value at the first index
function getUniqueLettersFrom(string){
    var tempObject = {};//Creates an empty object
    for (var i = 0; i < string.length; i++){
        if(tempObject[string[i]] === undefined){
            //does that thing(key) exist in the object?
            tempObject[string[i]] = 1;//{'h':1, 'e':1, 'l':2}
            //if it doesnt then create it and set equel to 1
        }
    else {
        // if already there, add to it
        tempObject[string[i]] += 1;
    }
    }
    var finalString = ""
    for (var key in tempObject){//This is used because its an object
        //goes through each key in the object
        if(tempObject[key] === 1){
            // tests whether the value = 1 and if so adds the value to final string
            finalString += key
        }
    }
    return finalString
}
console.log(getUniqueLettersFrom("Hello"));

//given 2 arrays, generate an object, lengths are equel. Key from array 1 and value from array 2.
function zipArrays(arr1, arr2){
    var emptyObject = {}
    for(var i = 0; i < arr1.length; i++){
        emptyObject[arr1[i]] = arr2[i];
    }
    return emptyObject;
}
console.log(zipArrays([1,"fuck",144], ["yikes!", 13, 100]));

