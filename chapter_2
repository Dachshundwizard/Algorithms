function pushFront(arr, val){
    for(var i = arr.length; i > 0; i--){
        arr[i] = arr[i-1];
    }
    arr[0] = val;
    return arr;
}
console.log(pushFront([1,2,3,4], 4));

function pushFront(arr, y){
    for(var i = arr.length; i > 0; i++){
        arr[i] = arr[i-1];
    }
    arr[0] = y;
    return arr;
    console.log(arr);
}
pushFront([1,2,3,4], 15);
